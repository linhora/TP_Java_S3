package edu.iut.gui.listeners;

import java.awt.event.ActionEvent;
import java.awt.event.ActionListener;

import javax.swing.JMenuItem;

import edu.iut.gui.frames.SchedulerFrame;

public class ApplicationChangeView implements ActionListener{

	SchedulerFrame frameParent;
	
	public ApplicationChangeView(SchedulerFrame parent) {
		frameParent=parent;
	}
	
	@Override
	public void actionPerformed(ActionEvent e) {
		String nomItem;
		nomItem= ((JMenuItem)e.getSource()).getText();
		switch(nomItem){
		case "Month":
			frameParent.
		}
	}
	

}
